{
    "name": "@eslint-react/shared",
    "version": "0.5.1",
    "description": "",
    "homepage": "https://github.com/Rel1cx/eslint-react",
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-react/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-react.git",
        "directory": "packages/shared"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "keywords": [
        "eslint-config"
    ],
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "./package.json"
    ],
    "scripts": {
        "lint:type": "tsc --noEmit",
        "lint:publish": "pnpm publint",
        "build": "rollup -c rollup.config.ts --configPlugin swc3 && cp dist/index.d.ts dist/index.d.mts",
        "publish": "pnpm run build && pnpm run lint:publish"
    },
    "dependencies": {
        "@eslint-react/tools": "workspace:*",
        "@typescript-eslint/utils": "6.9.0",
        "dedent": "1.5.1",
        "deepmerge-ts": "5.1.0",
        "tslib": "2.6.2"
    }
}
