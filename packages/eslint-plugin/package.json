{
    "name": "@eslint-react/eslint-plugin",
    "version": "0.3.9",
    "description": "ESLint plugin for React and Preact function components with TypeScript, built from scratch (almost).",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "eslint-react",
        "react",
        "ts",
        "tsx",
        "typescript",
        "typescript-eslint",
        "typescript-react"
    ],
    "homepage": "https://github.com/Rel1cx/eslint-react",
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-react/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-react.git",
        "directory": "packages/eslint-plugin"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "files": [
        "./dist",
        "./package.json"
    ],
    "scripts": {
        "lint:type": "tsc --noEmit",
        "build": "rollup -c rollup.config.ts --configPlugin swc3",
        "build:docs": "bun run ./scripts/doc-generate.ts && eslint-doc-generator && dprint fmt docs/**/*.md",
        "publish": "pnpm run build && pnpm run test && pnpm publint",
        "test": "vitest --run --single-thread --no-threads"
    },
    "dependencies": {
        "@eslint-react/eslint-plugin-debug": "workspace:*",
        "@eslint-react/eslint-plugin-hooks": "workspace:*",
        "@eslint-react/eslint-plugin-jsx": "workspace:*",
        "@eslint-react/eslint-plugin-naming-convention": "workspace:*",
        "@eslint-react/eslint-plugin-react": "workspace:*",
        "@typescript-eslint/scope-manager": "6.9.0",
        "@typescript-eslint/type-utils": "6.9.0",
        "@typescript-eslint/types": "6.9.0",
        "@typescript-eslint/utils": "6.9.0"
    },
    "devDependencies": {
        "@eslint-react/ast": "workspace:*",
        "@eslint-react/component": "workspace:*",
        "@eslint-react/construction": "workspace:*",
        "@eslint-react/context": "workspace:*",
        "@eslint-react/create-element": "workspace:*",
        "@eslint-react/hooks": "workspace:*",
        "@eslint-react/jsx": "workspace:*",
        "@eslint-react/pragma": "workspace:*",
        "@eslint-react/render-prop": "workspace:*",
        "@eslint-react/shared": "workspace:*",
        "@eslint-react/tools": "workspace:*",
        "@eslint-react/types": "workspace:*",
        "@eslint-react/variable": "workspace:*",
        "@rizzzse/bimap": "1.2.1",
        "@typescript-eslint/rule-tester": "6.9.0",
        "dedent": "1.5.1",
        "deepmerge-ts": "5.1.0",
        "effect": "2.0.0-next.50",
        "rambda": "8.5.0",
        "ts-pattern": "5.0.5"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": ">=6.9.0",
        "eslint": ">=8.52.0",
        "typescript": ">=5.2.2"
    },
    "engines": {
        "bun": ">=1.0.4",
        "node": ">=18.15.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
